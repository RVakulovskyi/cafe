    <validationRules>
        <fullName>Additional_Information</fullName>
        <active>false</active>
        <description>Validation requirements for the Additional Information section</description>
        <errorConditionFormula>AND(OR
(AND(ISBLANK(TEXT(Construction_Work_Status__c)),
OR(ISPICKVAL(StageName,&quot;Qualified Lead&quot;),
ISPICKVAL(StageName,&quot;Initial App&quot;),
ISPICKVAL(StageName,&quot;LOI&quot;),
ISPICKVAL(StageName,&quot;Committed&quot;))),

AND(ISBLANK(TEXT(Project_Objective__c)),
OR(ISPICKVAL(StageName,&quot;Qualified Lead&quot;),
ISPICKVAL(StageName,&quot;Initial App&quot;),
ISPICKVAL(StageName,&quot;LOI&quot;),
ISPICKVAL(StageName,&quot;Committed&quot;))),

AND(ISBLANK(TEXT(Project_Type__c)),
OR(ISPICKVAL(StageName,&quot;Qualified Lead&quot;),
ISPICKVAL(StageName,&quot;Initial App&quot;),
ISPICKVAL(StageName,&quot;LOI&quot;),
ISPICKVAL(StageName,&quot;Committed&quot;)))),	
Validated_by_CF__c=TRUE)</errorConditionFormula>
        <errorDisplayField>Project_Type__c</errorDisplayField>
        <errorMessage>One of these fields in Add. Information is required and missing:   

Construction Status-Initial Engagement (Qual. Lead), 
Project Objective (Qual. Lead), 
Project Type (Qual. Lead)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Additional_Information_2</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
OR(AND(
ISBLANK(TEXT(Type_of_Lender__c)),
OR(ISPICKVAL(StageName,&quot;Initial App&quot;),
ISPICKVAL(StageName,&quot;LOI&quot;),
ISPICKVAL(StageName,&quot;Committed&quot;))),

AND(ISBLANK(TEXT(Type_of_Loan_First__c)),
OR(ISPICKVAL(StageName,&quot;LOI&quot;),
ISPICKVAL(StageName,&quot;Committed&quot;)))),

Validated_by_CF__c=TRUE)</errorConditionFormula>
        <errorDisplayField>Mortgage_Lender__c</errorDisplayField>
        <errorMessage>One of these fields in Add. Information is required and missing: 

Mortgage Lender (Initial App), 
Type of Lender (Initial App), 
Type of Mortgage (LOI).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dead_Lost_Dormant</fullName>
        <active>true</active>
        <description>Dead Lost Logic</description>
        <errorConditionFormula>AND(
NOT(ISNEW()),
OR(

